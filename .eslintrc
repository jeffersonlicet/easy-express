{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}